{
  "name": "ascii-ui2",
  "version": "0.1.0",
  "description": "Graphic terminal emulator for HTML canvas elements",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && scripts/build.sh",
    "dev": "tsc -w --sourceMap",
    "clean": "rimraf dist",
    "test": "npm run lint && npm run mocha",
    "lint": "tslint -p tsconfig.json -c tslint.yaml",
    "mocha": "nyc mocha -r ts-node/register -r tsconfig-paths/register src/**/__test/**/*.spec.ts",
    "mocha-dev": "nyc mocha -r ts-node/register --inspect-brk -r tsconfig-paths/register src/**/__test/**/*.spec.ts"
  },
  "keywords": [
    "deep",
    "extend",
    "merge",
    "recursive"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danikaze/ascii-ui2.git"
  },
  "homepage": "https://github.com/danikaze/ascii-ui2",
  "bugs": "https://github.com/danikaze/ascii-ui2/issues",
  "author": {
    "name": "danikaze",
    "email": "danikaze@gmail.com"
  },
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-lines-between-class-members": "^1.3.6",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.7.4",
    "typescript-tslint-plugin": "^0.5.5"
  },
  "dependencies": {}
}
