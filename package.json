{
  "name": "ascii-ui2",
  "version": "0.1.0",
  "description": "Graphic terminal emulator for HTML canvas elements",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && scripts/build.sh",
    "dev": "tsc -w --sourceMap",
    "clean": "rimraf dist",
    "test": "npm run lint && npm run showcase-static && npm run test-all",
    "lint": "tslint -p tsconfig.json -c tslint.yaml",
    "unit-test": "scripts/test.sh --ut",
    "unit-test-dev": "scripts/test.sh --ut --debug",
    "showcase": "ts-node node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "showcase-static": "ts-node node_modules/webpack/bin/webpack.js --env.static",
    "vr-test": "npm run showcase-static && scripts/test.sh --vr",
    "vr-test-dev": "scripts/test.sh --vr --debug",
    "test-all": "scripts/test.sh --all",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danikaze/ascii-ui2.git"
  },
  "homepage": "https://github.com/danikaze/ascii-ui2",
  "bugs": "https://github.com/danikaze/ascii-ui2/issues",
  "author": {
    "name": "danikaze",
    "email": "danikaze@gmail.com"
  },
  "license": "ISC",
  "private": true,
  "keywords": [
    "256 colors",
    "ansi",
    "ascii",
    "borders",
    "box",
    "button",
    "canvas",
    "color",
    "console",
    "curses",
    "cursor",
    "event",
    "focus",
    "grid",
    "html",
    "input field",
    "input",
    "manager",
    "menu",
    "mouse",
    "ncurses",
    "progress bar",
    "radio",
    "radiobutton",
    "render",
    "screenbuffer",
    "select",
    "shell",
    "style",
    "table",
    "term",
    "termcap",
    "terminal",
    "terminfo",
    "text",
    "textarea",
    "textbuffer",
    "tput",
    "true color",
    "ui",
    "widget",
    "xterm"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/git-revision-webpack-plugin": "^3.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.7",
    "@types/puppeteer": "^2.0.0",
    "@types/resemblejs": "^1.3.28",
    "@types/rimraf": "^2.0.3",
    "chai": "^4.2.0",
    "git-revision-webpack-plugin": "^3.0.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.10",
    "mkdirp": "^0.5.1",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "puppeteer": "^2.0.0",
    "resemblejs": "^3.2.3",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.20.1",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-lines-between-class-members": "^1.3.6",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.7.4",
    "typescript-tslint-plugin": "^0.5.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "extend-objects-only": "^1.0.1"
  }
}
